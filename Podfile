platform :ios, '10.0'

#source 'https://github.com/CocoaPods/Specs.git'
target '网易新闻' do

# 刷新
pod 'MJRefresh'

#网络请求
pod 'Alamofire'

#图片加载
pod 'Kingfisher'

#.webp格式图片价值
pod 'KingfisherWebP', '~> 0.7.0'

#自动布局
pod 'SnapKit'

#JSON解析
pod 'SwiftyJSON'
pod 'HandyJSON'

#播放器
pod 'BMPlayer'

#全屏返回
pod 'FDFullscreenPopGesture'

end
